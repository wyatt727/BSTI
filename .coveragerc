[run]
source = bsti_nessus
omit = 
    */tests/*
    */venv/*
    */__pycache__/*
    */__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
    warnings\.warn

[html]
directory = coverage_html_report 